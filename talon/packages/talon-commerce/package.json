    "faker": "^4.1.0",
    "jest": "^23.5.0",
    "lwc-components-lightning": "1.0.13-alpha",
    "lwc-engine": "0.33.9",
    "lwc-jest-preset": "0.33.9",
    "lwc-module-resolver": "0.33.9",
    "mkdirp": "^0.5.1",
    "talon-compiler": "^0.0.1",
    "talon-framework": "^0.0.1"
